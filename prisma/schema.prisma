
generator client {
  provider = "prisma-client"
  
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String      @id @default(cuid())
  email     String   @unique
  name      String?
  image    String?
  clerkId   String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

